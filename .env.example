# IA Board by Filippe™ - Curiso 1:1 Replica Configuration
# Complete system identical to Curiso project with IA Board branding

# AI API Keys (Required for real functionality - no placeholders)
OPENAI_API_KEY=sk-CHAVE_REAL_AQUI
ANTHROPIC_API_KEY=sk-ant-CHAVE_REAL_AQUI
GOOGLE_API_KEY=AIzaCHAVE_REAL_AQUI

# Curiso-Compatible Application Configuration
PUBLIC_APP_NAME="IA Board by Filippe"
PUBLIC_DEFAULT_MODEL="gpt-4o"
PUBLIC_ALLOWED_MODELS="gpt-4o,claude-3-sonnet-20240229,gemini-1.5-pro"
PUBLIC_BRANDING_COLOR="#00d9ff"

# Database Configuration
DATABASE_URL=postgresql://localhost:5432/ia_board

# Server Configuration
NODE_ENV=development
PORT=5000

# IA Board Specific Configuration
BRAND_NAME="IA Board by Filippe™"
BRAND_TAGLINE="Sistema Avançado de Canvas Inteligente"
BRAND_VERSION="Supremo v3.0"
DESKTOP_DOWNLOAD_URL="https://github.com/FeehMatheus/ia-board/releases"

# Advanced Features
ENABLE_REAL_AI=true
ENABLE_VIDEO_GENERATION=true
ENABLE_EXPORT_FUNCTIONS=true
ENABLE_TEMPLATES=true
ENABLE_CONNECTIONS=true

# Canvas Configuration
DEFAULT_CANVAS_GRID=true
DEFAULT_CANVAS_SCALE=1.0
MAX_NODES_PER_CANVAS=50
ENABLE_COLLABORATIVE_EDITING=false

# Security
SESSION_SECRET=sua_chave_secreta_super_segura_aqui
JWT_SECRET=sua_jwt_secret_aqui

# Analytics and Monitoring
ENABLE_ANALYTICS=true
ANALYTICS_PROVIDER="internal"

# Performance
CACHE_DURATION=3600
MAX_REQUEST_SIZE="10mb"
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=900000